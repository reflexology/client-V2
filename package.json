{
  "name": "client",
  "version": "0.1.0",
  "private": false,
  "main": "index.tsx",
  "license": "MIT",
  "dependencies": {
    "antd": "4.4.1",
    "axios": "^0.19.2",
    "axios-auth-refresh": "2.2.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-highlight-words": "^0.16.0",
    "react-input-mask": "^2.0.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1"
  },
  "scripts": {
    "lint": "yarn lint:script && yarn lint:style",
    "lint-fix": "yarn lint-fix:script && yarn lint-fix:style",
    "lint:script": "eslint --ext js,ts,tsx src",
    "lint:style": "stylelint src/**/*.scss --syntax scss",
    "lint-fix:script": "yarn lint:script --fix",
    "lint-fix:style": "yarn lint:style --fix",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "precommit": "pretty-quick --staged",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy": "netlify deploy --prod",
    "prettier:fix": "prettier --write src/**/*",
    "bundlesize": "bundlesize"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-centered": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@types/axios": "^0.14.0",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "26.0.4",
    "@types/node": "14.0.19",
    "@types/react": "16.9.41",
    "@types/react-dom": "16.9.8",
    "@types/react-highlight-words": "^0.16.1",
    "@types/react-input-mask": "^2.0.5",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-test-renderer": "^16.9.2",
    "@types/storybook__react": "^5.2.1",
    "@types/yup": "^0.29.1",
    "@welldone-software/why-did-you-render": "^4.2.5",
    "babel-loader": "^8.1.0",
    "bundlesize": "^0.18.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "husky": "^4.2.5",
    "netlify-cli": "^2.58.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "react-test-renderer": "^16.13.1",
    "source-map-explorer": "^2.4.2",
    "stylelint": "^13.6.1",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "typescript": "3.9.6"
  },
  "bundlesize": [
    {
      "path": "build/**/main*.js",
      "maxSize": "50 kB"
    },
    {
      "path": "build/**/*.chunk.js",
      "maxSize": "500 kB"
    },
    {
      "path": "build/**/main*.css",
      "maxSize": "5 kB"
    },
    {
      "path": "build/**/*.chunk.css",
      "maxSize": "150 kB"
    }
  ]
}
