{
  "name": "client",
  "version": "0.1.0",
  "private": false,
  "main": "index.tsx",
  "license": "MIT",
  "dependencies": {
    "antd": "4.17.4",
    "axios": "0.24.0",
    "axios-auth-refresh": "2.0.2",
    "lodash.isequal": "^4.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-highlight-words": "^0.17.0",
    "react-input-mask": "^2.0.4",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "react-window": "^1.8.6",
    "recoil": "^0.5.2",
    "workbox-background-sync": "^6.4.2",
    "workbox-broadcast-update": "^6.4.2",
    "workbox-cacheable-response": "^6.4.2",
    "workbox-core": "^6.4.2",
    "workbox-expiration": "^6.4.2",
    "workbox-google-analytics": "^6.4.2",
    "workbox-navigation-preload": "^6.4.2",
    "workbox-precaching": "^6.4.2",
    "workbox-range-requests": "^6.4.2",
    "workbox-routing": "^6.4.2",
    "workbox-strategies": "^6.4.2",
    "workbox-streams": "^6.4.2"
  },
  "scripts": {
    "lint": "yarn lint:script && yarn lint:style",
    "lint-fix": "yarn lint-fix:script && yarn lint-fix:style",
    "lint:script": "eslint --ext js,ts,tsx src",
    "lint:style": "stylelint src/**/*.scss",
    "lint-fix:script": "yarn lint:script --fix",
    "lint-fix:style": "yarn lint:style --fix",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format:all": "prettier --write src/**/*",
    "format:staged": "pretty-quick --staged",
    "deploy": "netlify deploy --prod",
    "bundlesize": "bundlesize",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@types/lodash.isequal": "^4.5.5",
    "@types/node": "17.0.4",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/react-highlight-words": "^0.16.4",
    "@types/react-input-mask": "^3.0.1",
    "@types/react-router-dom": "^5.3.1",
    "@types/react-test-renderer": "^17.0.1",
    "@types/react-window": "^1.8.5",
    "@welldone-software/why-did-you-render": "^6.2.3",
    "bundlesize": "^0.18.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.0",
    "netlify-cli": "^8.4.2",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.45.1",
    "source-map-explorer": "^2.5.2",
    "stylelint": "^14.2.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "typescript": "4.5.4"
  },
  "bundlesize": [
    {
      "path": "build/**/*.js",
      "maxSize": "500 kB"
    },
    {
      "path": "build/**/*.css",
      "maxSize": "100 kB"
    }
  ],
  "engines": {
    "node": ">=16.0.0"
  }
}
