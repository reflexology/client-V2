{
  "name": "client",
  "version": "0.1.0",
  "private": false,
  "main": "index.tsx",
  "license": "MIT",
  "dependencies": {
    "antd": "4.0.4",
    "axios": "^0.19.2",
    "axios-auth-refresh": "2.2.4",
    "formik": "^2.1.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-highlight-words": "^0.16.0",
    "react-input-mask": "^2.0.4",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "yup": "^0.28.3"
  },
  "scripts": {
    "lint": "yarn lint:script && yarn lint:style",
    "lint-fix": "yarn lint-fix:script && yarn lint-fix:style",
    "lint:script": "eslint --ext js,ts,tsx src",
    "lint:style": "stylelint src/**/*.scss --syntax scss",
    "lint-fix:script": "yarn lint:script --fix",
    "lint-fix:style": "yarn lint:style --fix",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "precommit": "pretty-quick --staged",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy": "netlify deploy --prod",
    "d": "prettier --write src/**/*"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-centered": "^5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/addons": "^5.3.17",
    "@storybook/react": "^5.3.17",
    "@types/axios": "^0.14.0",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "25.1.4",
    "@types/node": "13.9.3",
    "@types/react": "16.9.25",
    "@types/react-dom": "16.9.5",
    "@types/react-highlight-words": "^0.16.1",
    "@types/react-input-mask": "^2.0.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-test-renderer": "^16.9.2",
    "@types/storybook__react": "^5.2.1",
    "@types/yup": "^0.26.33",
    "babel-loader": "^8.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-simple-import-sort": "^5.0.2",
    "husky": "^4.2.3",
    "netlify-cli": "^2.40.0",
    "node-sass": "^4.13.1",
    "prettier": "^2.0.1",
    "pretty-quick": "^2.0.1",
    "react-test-renderer": "^16.13.1",
    "source-map-explorer": "^2.4.1",
    "stylelint": "^13.2.1",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "typescript": "3.8.3"
  }
}
